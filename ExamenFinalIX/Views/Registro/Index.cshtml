@model IEnumerable<ExamenFinalIX.Models.Registro>

@{
    ViewBag.Title = "REPORTE POR GAMA";
}

<h2>REPORTE POR GAMA</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Telefono1.gama)
        </th>


        <th>       
            @Html.DisplayNameFor(model => model.total)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaHora)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>

            @Html.DisplayFor(modelItem => item.Telefono1.Gama1.gama1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.total)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaHora)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id_registro }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id_registro }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id_registro })
        </td>
    </tr>
    }

</table>

<br>
<br>
<p><a class="btn btn-danger" href="https://localhost:44376/Home/Reporte">Regresar</a></p>
